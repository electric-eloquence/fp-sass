    "pattern",
    "pre-processor",
    "preprocessor",
    "prototype",
    "prototyper",
    "prototyping",
    "sass",
    "template"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/electric-eloquence/fp-sass/issues"
  },
